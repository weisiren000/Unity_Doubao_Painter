# 豆包API密钥 - 请替换为您自己的API密钥
DOUBAO_API_KEY=your_api_key_here

# 文件夹路径配置 - 可以使用相对路径或绝对路径
# 相对路径是相对于Python目录的位置
SCREENSHOTS_DIR=../Screenshots
OUTPUTS_DIR=../Outputs
LOGS_DIR=../logs

# 豆包图像生成API配置
DOUBAO_MODEL=doubao-seedream-3-0-t2i-250415
DOUBAO_API_URL=https://ark.cn-beijing.volces.com/api/v3/images/generations

# 豆包视觉理解API配置
DOUBAO_VISION_MODEL=doubao-1.5-thinking-vision-pro-250428
DOUBAO_VISION_API_URL=https://ark.cn-beijing.volces.com/api/v3/chat/completions

# 可选配置 - 取消注释并修改以自定义行为
# STREAMLIT_PORT=8505                # Streamlit服务器端口
# STREAMLIT_ADDRESS=127.0.0.1        # Streamlit服务器地址
# CHECK_INTERVAL=2                   # 图片监控检查间隔（秒）
# DEBUG=True                         # 启用调试模式